{
  "name": "maplarge-google",
  "version": "1.0.0-alpha.2",
  "description": "",
  "main": "lib",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/AppGeo/appgeo_googlemaps.maplarge.git"
  },
  "scripts": {
    "build-js": "browserify -s maplarge . > dist/maplarge-google.js",
    "build-min": "uglifyjs dist/maplarge-google.js -mc > dist/maplarge-google.min.js",
    "build": "npm run build-js && npm run build-min"
  },
  "author": "Calvin W. Metcalf <cmetcalf@appgeo.com>",
  "license": "MIT",
  "homepage": "https://bitbucket.org/AppGeo/appgeo_googlemaps.maplarge#readme",
  "dependencies": {
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babelify": "^7.2.0",
    "create-hash": "^1.1.2",
    "sphericalmercator": "^1.0.4",
    "zoku": "^1.1.1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "plugins": [
            "transform-object-rest-spread",
            "babel-plugin-add-module-exports"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "browserify": "^12.0.1",
    "uglify-js": "^2.6.1"
  }
}
