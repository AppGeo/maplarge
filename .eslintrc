{
  "env": {
    "browser": true,
    es2022: true
  },
  "ecmaFeatures": {
    "modules": true
  },
    extends: [
    'eslint:recommended',
    ],
    parserOptions: {
    ecmaVersion: 'latest',
    sourceType: 'module',
  },
  "rules": {
    "indent": [2, 2],
    "brace-style": [2, "1tbs"],
    "quotes": [2, "single"],
    "no-console": 0,
    "no-shadow": 0,
    "no-use-before-define": [2, "nofunc"],
    "no-underscore-dangle": 0,
    "no-constant-condition": 0,
    "space-after-function-name": 0,
   "consistent-return": 0
  }
}
